    "body-parser": "^1.19.0",
    "concurrently": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.18",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.5.0",
    "react-hook-form": "^6.15.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
